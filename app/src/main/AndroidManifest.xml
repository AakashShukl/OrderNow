<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pay4free.in.ordernow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Model.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Model.signin" />
        <activity android:name=".Model.logup" />
        <activity
            android:name=".Model.Navigationmenu"
            android:label="@string/title_activity_navigationmenu"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".FoodList" />
        <activity android:name=".CategoryList" />
        <activity android:name=".Fooddetail" />
        <activity android:name=".Cart" />
        <activity android:name=".Model.OrderStatus"></activity>
    </application>

</manifest>